<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="friend" class="com.lec.ch04.ex3_scope.Friend">
		<property name="friendNo" value="1"/>
		<property name="name" value="홍길동"/>
		<property name="tel" value="010-1234-1234"/>
	</bean>
	<bean id="friendProto" class="com.lec.ch04.ex3_scope.Friend" scope="prototype">
		<property name="friendNo" value="2"/>
		<property name="name" value="신길동"/>
		<property name="tel" value="010-1212-1212"/>
	</bean>
</beans>
